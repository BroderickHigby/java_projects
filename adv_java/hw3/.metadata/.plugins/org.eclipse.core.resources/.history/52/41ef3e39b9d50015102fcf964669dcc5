
public class Student 
 {
 	private String name;
 	
 	public Student()
 	{
 		this.name = "";
 	}
 	
 	public Student(String name)
 	{
 		this.name = name;
 	}
 	
 	public void setName(String name)
 	{
 		this.name = name;
 	}
 	
 	public String getName()
 	{
 		return this.name;
 	}
 	
 	public String toString()
 	{
 		return "Student name: " + this.name + "\n";
 	}
 }
